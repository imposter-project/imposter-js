{
  "name": "@imposter-js/imposter",
  "version": "2.3.0",
  "description": "Run integrated tests with Imposter.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imposter-project/imposter-js.git"
  },
  "main": "cjs/index.cjs",
  "module": "esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.cjs"
  },
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && rollup -c && cp package.json README.md dist/",
    "clean": "rimraf ./dist",
    "release": "npm test && npm run build && npm publish --access public",
    "test": "jest"
  },
  "files": [
    "**/*.js",
    "**/*.cjs",
    "**/*.js.map",
    "**/*.d.ts"
  ],
  "keywords": [
    "openapi",
    "testing",
    "integration",
    "imposter",
    "mock"
  ],
  "author": "Pete Cornish",
  "license": "Apache-2.0",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "axios": "^1.8.1",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
